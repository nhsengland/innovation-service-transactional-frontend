{
  "name": "app",
  "version": "1.0.1",
  "scripts": {
    "ng": "ng",
    "start:dev:spa": "ng serve --host 0.0.0.0",
    "start:dev:ssr": "ng run app:serve-ssr",
    "build:spa": "ng build --prod",
    "test": "./node_modules/.bin/jest --runInBand --coverage --silent",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "serve:ssr": "node dist/app/server/main.js",
    "build:ssr": "ng build --prod --base-href /transactional/ --deploy-url /transactional/static/ && ng run app:server:production && npm run copyconfig",
    "prerender": "ng run app:prerender",
    "copyconfig": "cp web.config dist/app"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.1.2",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "~11.1.2",
    "@angular/compiler": "~11.1.2",
    "@angular/core": "~11.1.2",
    "@angular/forms": "~11.1.2",
    "@angular/material": "^11.2.13",
    "@angular/platform-browser": "~11.1.2",
    "@angular/platform-browser-dynamic": "~11.1.2",
    "@angular/platform-server": "~11.1.2",
    "@angular/router": "~11.1.2",
    "@nguniversal/express-engine": "^11.1.1",
    "@ngx-translate/core": "^13.0.0",
    "applicationinsights": "~2.0.0",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.15.2",
    "express-session": "^1.17.1",
    "govuk-frontend": "^3.11.0",
    "helmet": "^4.6.0",
    "jest-junit": "^12.0.0",
    "jsonapi-serializer": "^3.6.7",
    "jspdf": "^2.3.1",
    "lodash": "^4.17.21",
    "multer": "^1.4.2",
    "ngx-cookie-service": "^12.0.0",
    "ngx-dropzone": "^3.0.0",
    "ngx-logger": "^4.1.9",
    "nhsuk-frontend": "^5.0.0",
    "passport-azure-ad": "^4.3.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "xhr2": "^0.2.1",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1101.3",
    "@angular/cli": "~11.1.3",
    "@angular/compiler-cli": "~11.1.2",
    "@nguniversal/builders": "^11.1.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.0",
    "@types/express-session": "^1.17.3",
    "@types/jest": "^26.0.20",
    "@types/jsonapi-serializer": "^3.6.3",
    "@types/lodash": "^4.14.168",
    "@types/multer": "^1.4.5",
    "@types/node": "^12.20.4",
    "@types/passport-azure-ad": "^4.0.8",
    "codelyzer": "^6.0.0",
    "dotenv": "^8.2.0",
    "husky": "^1.0.0-rc.13",
    "jest": "^26.6.3",
    "jest-preset-angular": "^8.3.2",
    "karma-coverage": "~2.0.3",
    "protractor": "~7.0.0",
    "ts-jest": "^26.5.2",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.2"
  },
